<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXTextField?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="200.0" prefWidth="400.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.csa.ChatClient.IPAddressController">
   <children>
      <Label layoutX="93.0" layoutY="14.0" text="Please enter an IP Address:">
         <font>
            <Font size="18.0" />
         </font>
      </Label>
      <JFXTextField fx:id="txtIPAddress" layoutX="101.0" layoutY="72.0" promptText="IP Address">
         <font>
            <Font size="18.0" />
         </font>
      </JFXTextField>
      <Button fx:id="btnContinue" layoutX="153.0" layoutY="147.0" mnemonicParsing="false" onAction="#processIPAddress" text="Continue">
         <font>
            <Font size="18.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
